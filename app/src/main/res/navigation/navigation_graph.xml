<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/userRegistration">

    <fragment
        android:id="@+id/userRegistration"
        android:name="com.example.theelephant.View.Fragments.UserRegistration"
        android:label="fragment_user_registration"
        tools:layout="@layout/fragment_user_registration" >
        <action
            android:id="@+id/action_userRegistration_to_userAuthorization"
            app:destination="@id/userAuthorization" />
        <action
            android:id="@+id/action_userRegistration_to_navigationButton2"
            app:destination="@id/navigation_graph_2" />
    </fragment>
    <fragment
        android:id="@+id/userAuthorization"
        android:name="com.example.theelephant.View.Fragments.UserAuthorization"
        android:label="fragment_user_authorization"
        tools:layout="@layout/fragment_user_authorization" />
    <navigation android:id="@+id/navigation_graph_2"
        app:startDestination="@id/navigationButton2">
        <fragment
            android:id="@+id/navigationButton2"
            android:name="com.example.theelephant.View.Fragments.NavigationButton"
            android:label="fragment_navigation_button"
            tools:layout="@layout/fragment_navigation_button">
            <action
                android:id="@+id/action_navigationButton2_to_calendarRecording"
                app:destination="@id/calendarRecording" />
            <action
                android:id="@+id/action_navigationButton2_to_personalAccount"
                app:destination="@id/personalAccount" />
        </fragment>
        <fragment
            android:id="@+id/calendarRecording"
            android:name="com.example.theelephant.View.Fragments.CalendarRecording"
            android:label="fragment_calendar_recording"
            tools:layout="@layout/fragment_calendar_recording" >
            <action
                android:id="@+id/action_calendarRecording_to_navigationButton2"
                app:destination="@id/navigationButton2" />
        </fragment>
        <fragment
            android:id="@+id/personalAccount"
            android:name="com.example.theelephant.View.Fragments.PersonalAccount"
            android:label="fragment_personal_account"
            tools:layout="@layout/fragment_personal_account" >
            <action
                android:id="@+id/action_personalAccount_to_navigationButton2"
                app:destination="@id/navigationButton2" />
        </fragment>
    </navigation>
</navigation>